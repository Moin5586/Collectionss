    static void main(string[] args)
    {
        List<int> numbers = new List<int>();
        numbers.Add(10);
        numbers.Add(20);
        numbers.Add(30);
        numbers.Add(40);
        numbers.Add(50);
        numbers.Add(60);
        Console.WriteLine("all numbers are:");
        foreach (int integers in numbers)
        {
            Console.WriteLine(integers);

        }
        numbers.Remove(30);
        if (numbers.Contains(60))
        {
            Console.WriteLine("last nummber!!!");
        }
        numbers.Sort();
        Console.WriteLine("after applying the changes and after sorting:");
        foreach (int integer in numbers)
        {
            Console.WriteLine(integer);
        }
        numbers.Count();
        Console.WriteLine("total count of integers applying changes:" + numbers.Count);
        Console.ReadLine();
    }
